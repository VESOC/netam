datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

model Post {
    id          String      @id
    title       String
    description String
    date        String
    category    String[]
    coverImage  String
    series      String
    readingTime Int
    content     String
    published   Boolean  @default(false)
}
